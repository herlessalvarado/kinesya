{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "lib": ["es6"],
    "noUnusedLocals": true,
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "esModuleInterop":true,
    "moduleResolution": "node",
    "types": ["reflect-metadata"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true

  },
  "compileOnSave": true,
  "include": [
    "src", "ioc"
  ]
}